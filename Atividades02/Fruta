package org.example;

public class Fruta {
    private String nome;
    private String cor;

    public Fruta(String nome, String cor){
        this.nome = nome;
        this.cor = cor;
    }

    public void amadurecer() {
        System.out.println("A" + nome + "esta madura");
    }


    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public String getCor() {
        return  cor;
    }

    public void setCor(String cor) {
        this.cor = cor;
    }
}

