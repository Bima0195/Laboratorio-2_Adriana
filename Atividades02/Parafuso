package org.example;

public class Parafuso {

    private String medida;
    private String tipo;

    public Parafuso(String medida, String tipo) {
        this.medida = medida;
        this.tipo = tipo;
    }


    public String getMedida() {
        return medida;
    }
    public void setMedida(String medida) {
        this.medida = medida;
    }

    public String getTipo() {
        return tipo;
    }

    public void setTipo(String tipo) {
        tipo = tipo;
    }
}

